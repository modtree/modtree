{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "JSX": "readonly",
    "globalThis": "readonly"
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@next/next/recommended",
    "prettier"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "@next/next/no-html-link-for-pages": 0,
    "@typescript-eslint/no-explicit-any": 0,
    "react/react-in-jsx-scope": 0,
    "@typescript-eslint/no-unused-vars": 0,
    "no-unused-vars": 0
  }
}
