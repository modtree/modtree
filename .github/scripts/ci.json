{
  "install": {
    "command": "yarn --frozen-lockfile --silent"
  },
  "build": {
    "command": "yarn build",
    "args": {
      "ci": [],
      "no_cache": ["--skip-nx-cache"]
    }
  },
  "test": {
    "projects": [
      "repos:test:api",
      "repos:test:user",
      "repos:test:degree",
      "repos:test:graph",
      "repos:test:module",
      "repos:test:module-condensed",
      "repos:test:module-full",
      "repos:test:pull",
      "web:test",
      "utils:test",
      "docs:test",
      "server:test",
      "sql:test"
    ],
    "dont-run": [],
    "command": "yarn nx run",
    "args": {
      "ci": ["--coverage", "--runInBand"],
      "no_cache": ["--coverage", "--runInBand", "--skip-nx-cache"]
    }
  }
}
