{
  "install": {
    "command": "yarn --frozen-lockfile --silent"
  },
  "build": {
    "command": "yarn build",
    "args": {
      "ci": [],
      "no_cache": ["--skip-nx-cache"]
    }
  },
  "test": {
    "projects": [
      "docs",
      "server",
      "web",
      "auto-docs",
      "integration-tests",
      "repo:module",
      "migrations",
      "repo:api",
      "repo:base",
      "repo:degree",
      "repo:graph",
      "repo:module-condensed",
      "repo:module-full",
      "repo:pull",
      "repo:user",
      "sql",
      "test-env",
      "typeorm-config",
      "types",
      "utils"
    ],
    "dont-run": [],
    "command": "yarn test",
    "args": {
      "ci": ["--coverage", "--runInBand"],
      "no_cache": ["--coverage", "--runInBand"]
    }
  }
}
