{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/typeorm-config/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "updateBuildableProjectDepsInPackageJson": true,
        "outputPath": "dist/libs/typeorm-config",
        "main": "libs/typeorm-config/src/index.ts",
        "tsConfig": "libs/typeorm-config/tsconfig.lib.json",
        "assets": ["libs/typeorm-config/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/typeorm-config/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/typeorm-config"],
      "options": {
        "jestConfig": "libs/typeorm-config/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "generate-migration": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "ts-node --project tsconfig.lib.json -r tsconfig-paths/register ../../node_modules/.bin/typeorm migration:generate --pretty -d src/migration.config.ts src/migrations/untitled",
        "cwd": "libs/typeorm-config"
      }
    },
    "test-connect": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "ts-node --project tsconfig.lib.json -r tsconfig-paths/register src/test-connect.ts",
        "cwd": "libs/typeorm-config"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["!test-env"]
}
