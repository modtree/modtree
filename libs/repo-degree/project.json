{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/repo-degree/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "updateBuildableProjectDepsInPackageJson": true,
        "outputPath": "dist/libs/repo-degree",
        "main": "libs/repo-degree/src/index.ts",
        "tsConfig": "libs/repo-degree/tsconfig.lib.json",
        "assets": ["libs/repo-degree/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/repo-degree/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/repo-degree"],
      "options": {
        "jestConfig": "libs/repo-degree/jest.config.ts",
        "passWithNoTests": true,
        "runInBand": true
      }
    }
  },
  "tags": []
}
