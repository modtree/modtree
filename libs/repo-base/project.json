{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/repo-base/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "updateBuildableProjectDepsInPackageJson": true,
        "outputPath": "dist/libs/repo-base",
        "main": "libs/repo-base/src/index.ts",
        "tsConfig": "libs/repo-base/tsconfig.lib.json",
        "assets": ["libs/repo-base/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/repo-base/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/repo-base"],
      "options": {
        "jestConfig": "libs/repo-base/jest.config.ts",
        "passWithNoTests": true,
        "runInBand": true
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["!test-env"]
}
