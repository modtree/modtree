{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/test-env/src",
  "projectType": "library",
  "targets": {
    "setup": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/libs/test-env/src/hello.js"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "updateBuildableProjectDepsInPackageJson": true,
        "outputPath": "dist/libs/test-env",
        "main": "libs/test-env/src/index.ts",
        "tsConfig": "libs/test-env/tsconfig.lib.json",
        "assets": ["libs/test-env/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/test-env/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/test-env"],
      "options": {
        "jestConfig": "libs/test-env/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
