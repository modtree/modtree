{
  "root": true,
  "overrides": [
    {
      "plugins": ["jsdoc"],
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_$" }],
          "error",
          {
            "enforceBuildableLibDependency": true,
            "allow": [],
            "depConstraints": [
              {
                "sourceTag": "*",
                "onlyDependOnLibsWithTags": ["*"]
              }
            ]
          }
        ]
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "extends": [],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "extends": [],
      "rules": {}
    },
    {
      "files": ["*.test.ts", "*.spec.ts"],
      "extends": [],
      "rules": {
        "@typescript-eslint/no-non-null-assertion": 0
      }
    }
  ]
}
