{
  "name": "database",
  "version": "0.0.0",
  "license": "MIT",
  "description": "modtree database with MySQL",
  "type": "commonjs",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.5.0",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.11.10",
    "babel-jest": "^28.1.0",
    "jest": "^28.1.0",
    "modtree-config": "*",
    "ts-jest": "^28.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@inquirer/input": "^0.0.20-alpha.0",
    "axios": "^0.27.2",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.0",
    "inquirer": "^8.2.4",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.6"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint --fix --ext .js,.ts .",
    "test": "NODE_ENV=test jest --config ./tests/configs/ci.config.js --detectOpenHandles",
    "test:ci": "NODE_ENV=test jest --config ./tests/configs/ci.config.js --runInBand",
    "test:entity": "NODE_ENV=test jest --config ./tests/configs/entity.config.js --json --outputFile ./.logs/entity.json",
    "dev": "node lib",
    "dump": "ts-node ./src/api/sql/dump.ts",
    "restore": "ts-node ./src/api/sql/restore.ts",
    "restore:t": "ts-node ./src/api/sql/restoreTest.ts",
    "au": "ts-node ./src/api/user/add.ts",
    "ctm": "ts-node ./src/api/user/canTakeModule.ts",
    "lm": "ts-node ./src/api/list/module.ts",
    "lmc": "ts-node ./src/api/list/moduleCode.ts",
    "fom": "ts-node ./src/api/nusmods/fetch/oneModule.ts",
    "fmc": "ts-node ./src/api/nusmods/fetch/moduleCondensed.ts",
    "pmc": "ts-node ./src/api/nusmods/pull/moduleCondensed.ts",
    "reset:all": "ts-node ./src/api/sql/reset/all.ts",
    "reset:mc": "ts-node ./src/api/sql/reset/moduleCondensed.ts",
    "reset:m": "ts-node ./src/api/sql/reset/module.ts",
    "reset:d": "ts-node ./src/api/sql/reset/degree.ts",
    "ad": "ts-node ./src/api/degree/add.ts",
    "sd": "ts-node ./src/api/degree/show.ts",
    "mcg": "ts-node ./src/api/moduleCondensed/get",
    "mcf": "ts-node ./src/api/moduleCondensed/fetch",
    "mg": "ts-node ./src/api/module/get",
    "mgc": "ts-node ./src/api/module/get/codes",
    "pm": "ts-node ./src/api/module/pull.ts"
  }
}
