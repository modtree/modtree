{
  "name": "database",
  "version": "0.0.0",
  "license": "MIT",
  "description": "modtree database with MySQL",
  "type": "commonjs",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.5.0",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.11.10",
    "babel-jest": "^28.1.0",
    "jest": "^28.1.0",
    "modtree-config": "*",
    "ts-jest": "^28.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@inquirer/input": "^0.0.20-alpha.0",
    "axios": "^0.27.2",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.0",
    "inquirer": "^8.2.4",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.6"
  },
  "scripts": {
    "build": "tsc",
    "test": "yarn test:ci --detectOpenHandles",
    "test:ci": "NODE_ENV=test jest --coverage --config ./tests/configs/ci.config.js --runInBand",
    "test:k": "NODE_ENV=test jest --config ./tests/configs/khang.config.js --runInBand",
    "test:entity": "NODE_ENV=test jest --config ./tests/configs/entity.config.js --json --outputFile ./.logs/entity.json",
    "lint": "eslint --fix --ext .js,.ts .",
    "lint:prettier": "prettier --ignore-path .gitignore --write . && yarn lint",
    "dump": "ts-node ./src/api/sql/dump.ts",
    "restore": "ts-node ./src/api/sql/restore.ts",
    "restore:t": "ts-node ./src/api/sql/restoreTest.ts",
    "us": "ts-node ./src/api/user/save.ts",
    "uctm": "ts-node ./src/api/user/canTakeModule.ts",
    "reset:all": "ts-node ./src/api/sql/reset/all.ts",
    "reset:mc": "ts-node ./src/api/sql/reset/moduleCondensed.ts",
    "reset:m": "ts-node ./src/api/sql/reset/module.ts",
    "reset:d": "ts-node ./src/api/sql/reset/degree.ts",
    "mcg": "ts-node ./src/api/moduleCondensed/get.ts",
    "mcf": "ts-node ./src/api/moduleCondensed/fetch.ts",
    "mg": "ts-node ./src/api/module/get/index.ts",
    "mgc": "ts-node ./src/api/module/get/codes.ts",
    "pm": "ts-node ./src/api/module/pull.ts",
    "typeorm_ds": "typeorm-ts-node-esm -d ./src/data-source.ts",
    "migration": "yarn typeorm_ds migration:generate ./src/migrations/migrationName"
  }
}
