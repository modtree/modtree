{
  "name": "database",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "description": "modtree database with MySQL",
  "main": "./dist/index.js",
  "type": "commonjs",
  "devDependencies": {
    "@jest/test-sequencer": "^28.1.0",
    "@jest/types": "^28.1.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.5.0",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.11.10",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "eslint-config-modtree": "*",
    "eslint-plugin-jest": "^26.2.2",
    "jest": "^28.1.0",
    "start-server-and-test": "^1.14.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@inquirer/input": "^0.0.20-alpha.0",
    "axios": "^0.27.2",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "inquirer": "^8.2.4",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.16",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.6"
  },
  "scripts": {
    "h0": "─────────────────────────────────────────────────────── ci ",
    "ci:build": "yarn lint:no-fix && yarn build",
    "ci:test:database": "bash ./tests/scripts.sh database",
    "ci:test:server": "bash ./tests/scripts.sh server",
    "ci:test:all": "yarn ci:test:database && yarn ci:test:server",
    "h1": "─────────────────────────────────────────────── core utils ",
    "start": "node dist/server",
    "start:test": "NODE_ENV=test yarn start",
    "build": "rm -rf ./dist && tsc --build tsconfig.build.json",
    "dev": "yarn build && yarn start",
    "dev:test": "yarn build && yarn start:test",
    "graph": "madge --image graph.png",
    "default:env": "sh setup.sh",
    "jest": "NODE_ENV=test jest",
    "h2": "───────────────────────────────────────────── test configs ",
    "tconf:database": "yarn jest -c ./tests/configs/database.ts",
    "tconf:server": "yarn jest -c ./tests/configs/server.ts",
    "tconf:pull": "yarn jest -c ./tests/configs/pull.ts",
    "tconf:k": "yarn jest -c ./tests/configs/k.ts",
    "tconf:w": "yarn jest -c ./tests/configs/w.ts",
    "tconf:all": "yarn jest -c ./tests/configs/all.ts",
    "tconf:group": "yarn jest -c ./tests/configs/group.ts",
    "h3": "───────────────────────────────────────────── test aliases ",
    "test:database": "yarn tconf:database",
    "test:ci": "yarn test:database",
    "test:pull": "yarn tconf:pull",
    "test:k": "yarn tconf:k",
    "test:kd": "yarn tconf:k --detectOpenHandles",
    "test:w": "yarn tconf:w",
    "test:all": "yarn tconf:all",
    "test:s": "yarn tconf:server",
    "test": "yarn tconf:group",
    "test:d": "yarn tconf:group degree",
    "test:m": "yarn tconf:group module",
    "test:mc": "yarn tconf:group module-condensed",
    "test:g": "yarn tconf:group graph",
    "test:u": "yarn tconf:group user",
    "test:b": "yarn tconf:group base --silent=false",
    "h4": "────────────────────────────────────────────────── linting ",
    "lint": "eslint --fix --ext .js,.ts .",
    "lint:no-fix": "eslint --ext .js,.ts .",
    "h5": "───────────────────────────────────────────── sql commands ",
    "dump": "ts-node ./src/api/sql/dump.ts",
    "restore": "ts-node ./src/api/sql/restore.ts",
    "restore:test": "NODE_ENV=test ts-node ./src/api/sql/restore.ts",
    "restore:server-test": "NODE_ENV=test ts-node ./tests/server/restore.ts",
    "restore:file": "ts-node ./src/api/sql/restore-from-file.ts",
    "restore:default": "ts-node ./src/api/sql/restore-default.ts",
    "reset:all": "ts-node ./src/api/sql/reset/all.ts",
    "reset:mc": "ts-node ./src/api/sql/reset/moduleCondensed.ts",
    "reset:m": "ts-node ./src/api/sql/reset/module.ts",
    "reset:d": "ts-node ./src/api/sql/reset/degree.ts",
    "h6": "──────────────────────────────────────────────── migration ",
    "typeorm:ds": "typeorm-ts-node-esm -d ./src/config/index.ts",
    "mc": "yarn typeorm:ds schema:log",
    "mg": "yarn typeorm:ds migration:generate ./src/migrations/migration-name",
    "mr": "yarn typeorm:ds migration:run",
    "h7": "───────────────────────────────────────────────── yarn API ",
    "module:get-codes": "ts-node ./src/api/module/get-codes.ts",
    "module:get": "ts-node ./src/api/module/get.ts",
    "module:pull": "ts-node ./src/api/module/pull.ts",
    "module:fetch-one": "ts-node ./src/api/module/fetch-one.ts",
    "module:find-by-codes": "ts-node ./src/api/module/find-by-codes.ts",
    "module:find-by-faculty": "ts-node ./src/api/module/find-by-faculty.ts",
    "modcon:fetch": "ts-node ./src/api/module-condensed/fetch.ts",
    "modcon:pull": "ts-node ./src/api/module-condensed/pull.ts",
    "modcon:get-codes": "ts-node ./src/api/module-condensed/get-codes.ts",
    "modcon:json": "ts-node ./src/api/module-condensed/json.ts"
  }
}
