{
  "name": "database",
  "version": "0.0.0",
  "license": "MIT",
  "description": "modtree database with MySQL",
  "type": "commonjs",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.5.0",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.11.10",
    "babel-jest": "^28.1.0",
    "jest": "^28.1.0",
    "modtree-config": "*",
    "ts-jest": "^28.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@inquirer/input": "^0.0.20-alpha.0",
    "axios": "^0.27.2",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.0",
    "inquirer": "^8.2.4",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.6"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint --fix --ext .js,.ts .",
    "test": "NODE_ENV=test jest --config ./tests/configs/main.config.js",
    "test:fetch": "NODE_ENV=test jest --config ./tests/configs/fetch.config.js",
    "test:verbose": "NODE_ENV=test jest --config ./tests/configs/main-verbose.config.js",
    "test:v": "yarn test:verbose",
    "dev": "node lib",
    "dump": "ts-node ./src/api/sql/dump",
    "restore": "ts-node ./src/api/sql/restore",
    "start": "ts-node src/index",
    "typeorm": "typeorm-ts-node-commonjs",
    "au": "ts-node ./src/api/user/add",
    "ctm": "ts-node ./src/api/user/canTakeModule",
    "ad": "ts-node ./src/api/degree/add",
    "lm": "ts-node ./src/api/list/module",
    "lmc": "ts-node ./src/api/list/moduleCode",
    "fm": "ts-node ./src/api/nusmods/fetch/module",
    "fom": "ts-node ./src/api/nusmods/fetch/oneModule",
    "fmc": "ts-node ./src/api/nusmods/fetch/moduleCondensed",
    "fnmc": "ts-node ./src/api/nusmods/fetch/newModuleCondensed",
    "pmc": "ts-node ./src/api/nusmods/pull/moduleCondensed",
    "pm": "ts-node ./src/api/nusmods/pull/module",
    "pmc:heroku": "NODE_ENV=heroku ts-node ./src/api/nusmods/pull/moduleCondensed",
    "reset:all": "ts-node ./src/api/sql/reset/all",
    "reset:mc": "ts-node ./src/api/sql/reset/moduleCondensed",
    "reset:m": "ts-node ./src/api/sql/reset/module"
  }
}
