{
  "pipeline": {
    "ci": {
      "dependsOn": ["^ci"],
      "outputs": ["dist/**", ".next/**", "lib/**"]
    },
    "build": {
      "dependsOn": ["^build", "lint"],
      "outputs": ["dist/**", ".next/**", "lib/**"]
    },
    "deploy": {
      "dependsOn": ["build", "lint"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
