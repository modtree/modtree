{
  "name": "modtree",
  "version": "0.5.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "nx run-many --target=serve --projects=web,server",
    "build": "nx run-many --all --target=build --parallel=4 --exclude=docs",
    "lint": "nx run-many --all --target=lint --parallel=4",
    "test": "node scripts/test.js",
    "test:a": "nx run-many --all --target=test --parallel=1 --exclude=auto-docs",
    "build:f": "yarn build --skip-nx-cache",
    "lint:f": "yarn lint --skip-nx-cache",
    "test:f": "yarn test --skip-nx-cache",
    "build:docs": "nx build docs",
    "e2e": "nx e2e web-e2e --watch",
    "format": "prettier --write .",
    "prepare": "bash ./scripts/yarn.sh setup",
    "i": "bash ./scripts/yarn.sh inverse",
    "heroku-postbuild": "yarn build:server --prod",
    "auto": "nx test auto-docs",
    "version": "auto-changelog -p && git add --all",
    "dev:server": "node apps/server/dev.js --watch",
    "dev:web": "nx serve web",
    "dev": "sh ./scripts/serve.sh",
    "build:server": "node apps/server/webpack.js --build",
    "start:server": "node dist/apps/server/bundle.js",
    "all:server": "yarn build:server && yarn start:server",
    "mg": "nx generate-migration typeorm-config"
  },
