{
  "name": "modtree-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "apps/backend/functions",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=database --filter=web",
    "build:db": "turbo run build --filter=database",
    "test:db": "turbo run test --filter=database",
    "lint": "turbo run lint",
    "dev": "turbo run dev --filter=database --filter=web --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "rm:logs": "find . -name *.log | xargs rm",
    "setup": "yarn && yarn build:db && yarn"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.18"
}
