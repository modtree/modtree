    "tailwindcss": "3.0.15",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "webpack": "5.65.0"
  },
  "workspaces": [
    "packages/**",
    "apps/**"
  ],
  "engines": {
    "npm": "8.x",
    "node": "16.x",
    "yarn": "1.x"
  },
  "packageManager": "yarn@1.22.18"
}
