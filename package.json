{
  "name": "modtree",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ci:build": "nx affected --target=build",
    "ci:build:s": "nx affected --target=build --skip-nx-cache",
    "build": "nx run-many --target=build --all",
    "build:s": "nx run-many --target=build --all --skip-nx-cache",
    "test": "yarn nx ci:test database",
    "lint": "turbo run lint",
    "dev": "nx run-many --target=dev --all",
    "dev:docs": "turbo run dev --filter=docs",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "setup": "yarn && yarn build:db && yarn default:env",
    "heroku-postbuild": "yarn build:db",
    "default:env": "turbo run default:env",
    "version": "auto-changelog -p && git add --all",
    "prepare": "bash ./scripts.sh setup",
    "graph": "nx graph --focus=database",
    "i": "bash ./scripts.sh inverse"
  },
  "private": true,
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.9.0",
    "@heroicons/react": "^1.0.6",
    "@inquirer/input": "^0.0.20-alpha.0",
    "@react-aria/ssr": "^3.2.0",
    "@reduxjs/toolkit": "^1.8.1",
    "auto-changelog": "^2.4.0",
    "axios": "^0.27.2",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.1",
    "inquirer": "^8.2.4",
    "next": "^12.1.6",
    "nextra": "2.0.0-alpha.50",
    "nextra-theme-docs": "2.0.0-alpha.53",
    "pg": "^8.7.3",
    "prismjs": "^1.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-flow-renderer": "^10.2.1",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@jest/test-sequencer": "^28.1.1",
    "@jest/types": "^28.1.1",
    "@nrwl/cli": "14.2.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "14.2.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@types/pg": "^8.6.5",
    "@types/prismjs": "^1.26.0",
    "@types/react": "17.0.37",
    "@types/supertest": "^2.0.12",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "10.2.6",
    "csstype": "2.6.2",
    "eslint": "8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsdoc": "^39.3.2",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
