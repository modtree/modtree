{
  "name": "modtree",
  "version": "0.8.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "────────────────────────────────────────────────────── dev ": [],
    "dev:server": "node apps/server/dev.js --watch",
    "dev:web": "yarn serve:web",
    "dev:docs": "bash scripts/docs/serve.sh",
    "build:docs": "bash scripts/docs/build.sh",
    "dev": "sh scripts/serve.sh",
    "build:server": "node apps/server/webpack.js --build",
    "start:server": "node dist/apps/server/bundle.js",
    "all:server": "yarn build:server && yarn start:server",
    "─────────────────────────────────────────────────── docker ": [],
    "up": "cd docker && ./up db",
    "down": "cd docker && ./down all",
    "────────────────────────────────────────────────── cypress ": [],
    "cy": "bash scripts/cypress.sh",
    "cy:build": "node libs/cy-reporter/webpack.js --build",
    "cy:dev": "node libs/cy-reporter/dev.js",
    "cy:ls": "node apps/web-e2e/reporters/list.js",
    "────────────────────────────────────────────────── testing ": [],
    "test": "bash scripts/run-tests/run.sh",
    "test:dev": "yarn test:build && yarn test",
    "test:scan": "ts-node scripts/run-tests/scan.ts",
    "test:build": "yarn test:scan && node scripts/run-tests/build.js",
    "e2e": "nx e2e web-e2e --watch",
    "e2e:prod": "nx e2e:prod web-e2e --watch",
    "e2e:dev": "nx e2e:dev web-e2e --watch",
    "──────────────────────────────────────────────── migration ": [],
    "mg": "bash scripts/migration/generate.sh",
    "mr": "bash scripts/migration/run.sh",
    "tc": "bash scripts/migration/test-connect.sh",
    "────────────────────────────────────────────── nx remnants ": [],
    "serve:web": "nx serve web",
    "build": "nx run-many --all --target=build --parallel=4",
    "lint": "nx run-many --all --target=lint --parallel=4",
    "─────────────────────────────────────────────── automation ": [],
    "version": "auto-changelog -p && git add --all",
    "i": "bash scripts/yarn.sh inverse || exit 0",
    "prepare": "bash scripts/yarn.sh setup || exit 0",
    "heroku-postbuild": "yarn build:server --prod"
  },
