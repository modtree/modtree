{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "apps/backend/functions",
    "packages/*"
  ],
  "scripts": {
    "back": "turbo run backend --filter=functions",
    "build": "turbo run build",
    "build:backend": "turbo run build --filter=functions",
    "base": "turbo run build --filter=modtree",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "setup": "yarn && yarn base && yarn"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.18"
}
